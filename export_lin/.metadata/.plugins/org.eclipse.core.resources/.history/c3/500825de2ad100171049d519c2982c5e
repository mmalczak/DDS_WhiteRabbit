#include <stdio.h>
#include <unistd.h>
#include <fcntl.h>

int main()
{
	int dh = open("/dev/mem", O_RDWR | O_SYNC); // Open /dev/mem which represents the whole physical memory
	if(dh<0)return -1;
	write(dh, "0x43C00000 32 0x01". 0x01);
	printf("dupa\n");

    return 0;
}
